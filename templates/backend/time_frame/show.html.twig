<!DOCTYPE html>

<title>TimeFrame</title>

{% block body %}
    <h1>TimeFrame</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ time_frame.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ time_frame.name }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ time_frame.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ time_frame.updatedBy }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ time_frame.createdAt ? time_frame.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ time_frame.updatedAt ? time_frame.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_time_frame_index') }}">back to list</a>

    <a href="{{ path('app_time_frame_edit', {'id': time_frame.id}) }}">edit</a>

    {{ include('backend/time_frame/_delete_form.html.twig') }}
{% endblock %}
