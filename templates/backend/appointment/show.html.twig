<!DOCTYPE html>

<title>Appointment</title>

{% block body %}
    <h1>Appointment</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ appointment.id }}</td>
            </tr>
            <tr>
                <th>VisitorFirstName</th>
                <td>{{ appointment.visitorFirstName }}</td>
            </tr>
            <tr>
                <th>VisitorLastName</th>
                <td>{{ appointment.visitorLastName }}</td>
            </tr>
            <tr>
                <th>StudentFirstName</th>
                <td>{{ appointment.studentFirstName }}</td>
            </tr>
            <tr>
                <th>StudentLastName</th>
                <td>{{ appointment.studentLastName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ appointment.email }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ appointment.token }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_appointment_index') }}">back to list</a>

    <a href="{{ path('app_appointment_edit', {'id': appointment.id}) }}">edit</a>

    {{ include('backend/appointment/_delete_form.html.twig') }}
{% endblock %}
